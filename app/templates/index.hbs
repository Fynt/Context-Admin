<h1>History</h1>

<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th>Date</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>
        {{gravatar-image email="keith@fynt.ca" size=16}}
        Keith did some junk
      </td>
      <td>{{relative-time date="2014-06-14 07:23:58"}}</td>
      <td></td>
    </tr>
  </tbody>
</table>

<div style="height: 500px; overflow: scroll-y">

{{table-component
  hasFooter=false
  columnsBinding="columns"
  contentBinding="content"
}}
</div>
